[
    {
        "ShortName": "ping_sweep",
        "CommandTemplate": "rand=`mktemp`; nmap -sn -PE {{CIDR}} -oG $rand; cat $rand | grep 'Status: Up' | cut -f 2 -d ' '",
        "IsActive": true,
        "Aggressiveness": 1,
        "Subject": "NetRange"
    },
    {
        "ShortName": "domain_resolution",
        "CommandTemplate": "dig +short {{Name}} | awk '{print \"{{Name}} IN A \" $1}'",
        "IsActive": false,
        "Aggressiveness": 1,
        "Subject": "Domain"
    },
    {
        "ShortName": "subfinder",
        "CommandTemplate": "subfinder -silent -d {{Name}}",
        "IsActive": false,
        "Aggressiveness": 0,
        "Filter": "(Domain.IsRegistrationDomain == true)",
        "Subject": "Domain"
    },
    {
